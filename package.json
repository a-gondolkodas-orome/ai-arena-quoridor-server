{
  "name": "ai-arena-quoridor",
  "version": "1.0.0",
  "description": "",
  "main": "dist/quoridor.js",
  "scripts": {
    "start": "npm run build && npm run run",
    "build": "npx tsc",
    "run": "node --es-module-specifier-resolution=node dist/src/quoridor.js",
    "lint": "npm run eslint:check && npm run prettier:check",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:check": "npx prettier --check src",
    "prettier:fix": "npx prettier --write src",
    "eslint:check": "npx eslint --report-unused-disable-directives src/*.ts",
    "eslint:fix": "npx eslint --fix --report-unused-disable-directives src/*.ts"
  },
  "browser": {
    "child_process": false
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^18.11.7",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "childprocess": "^2.0.2"
  }
}
